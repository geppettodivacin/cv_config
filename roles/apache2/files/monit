check file apache_bin with path /usr/sbin/apache2
	if failed checksum expect 61eb080ac7d9a348c1c41639c3c6421cc2b7e540 then alert
	if failed permission 755 then alert
	if failed uid root then alert
	if failed gid root then alert
	group apache

check process apache with pidfile /var/run/apache2.pid
	start program = "/usr/sbin/service apache2 start" with timeout 60 seconds
	stop program  = "/usr/sbin/service apache2 stop"
	if cpu > 60% for 2 cycles then alert
	if children > 250 then restart
	if loadavg(5min) greater than 10 for 8 cycles then stop
	if failed host account.collegiumv.org port 80 protocol http then alert
	if failed host newsletter.collegiumv.org port 80 protocol http then alert
	if failed host status.collegiumv.org port 80 protocol http then alert
	depends on apache_bin
	group apache
